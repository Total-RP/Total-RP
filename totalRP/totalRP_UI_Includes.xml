   <Size>
      <AbsDimension x="20" y="20"/>
    </Size>
    <Layers>
      <Layer level="BORDER">
        <Texture name="$parentIcon" file="Interface\Buttons\UI-EmptySlot-Disabled">
		  <Size>
			<AbsDimension x="20" y="20"/>
		  </Size>
		  <Anchors>
			<Anchor point="CENTER">
			  <Offset>
				<AbsDimension x="-1" y="0"/>
			  </Offset>
			</Anchor>
		  </Anchors>
		</Texture>
      </Layer>
	  <Layer level="OVERLAY">
		<FontString name="$parentUrl" inherits="NumberFontNormal" justifyH="RIGHT" hidden="true">
			<Anchors>
				<Anchor point="RIGHT">
					<Offset>
						<AbsDimension x="-6" y="-6"/>
					</Offset>
				</Anchor>
			</Anchors>
		</FontString>
		<FontString name="$parentNom" inherits="NumberFontNormal" justifyH="RIGHT" hidden="true">
			<Anchors>
				<Anchor point="RIGHT">
					<Offset>
						<AbsDimension x="-6" y="-6"/>
					</Offset>
				</Anchor>
			</Anchors>
		</FontString>
		<FontString name="$parentCategorie" inherits="NumberFontNormal" justifyH="RIGHT" hidden="true">
			<Anchors>
				<Anchor point="RIGHT">
					<Offset>
						<AbsDimension x="-6" y="-6"/>
					</Offset>
				</Anchor>
			</Anchors>
		</FontString>
	  </Layer>
    </Layers>
    <NormalTexture name="$parentNormalTexture" file="Interface\Buttons\UI-Quickslot2">
      <Size>
        <AbsDimension x="35" y="35"/>
      </Size>
      <Anchors>
        <Anchor point="CENTER">
          <Offset>
            <AbsDimension x="0" y="-1"/>
          </Offset>
        </Anchor>
      </Anchors>
    </NormalTexture>
    <PushedTexture file="Interface\Buttons\UI-Quickslot-Depress"/>
    <HighlightTexture alphaMode="ADD" file="Interface\Buttons\ButtonHilight-Square"/>
	<Scripts>
		<OnEnter>
			GameTooltip:SetOwner(this, "ANCHOR_TOPLEFT");
			GameTooltip:AddLine(getglobal(this:GetName().."Nom"):GetText(),0,1,0);
			GameTooltip:AddLine("Category : "..getglobal(this:GetName().."Categorie"):GetText(),0,1,0.5);
			GameTooltip:AddLine("Path : "..getglobal(this:GetName().."Url"):GetText(),1,1,1);
			GameTooltip:AddLine(" ",1,1,1);
			GameTooltip:AddLine("Right Click: Pre-listen to the sound",1,0.75,0);
			GameTooltip:AddLine("Left Click: Play sound |cff00ff00localy",0.25,1,0.25);
			GameTooltip:AddLine("Ctrl + Left Click: Play sound |cff00ffffglobaly",0.35,1,0.35);
			GameTooltip:Show();
		</OnEnter>
		<OnLeave>
			GameTooltip:Hide();
		</OnLeave>
		<OnLoad>
	       this:RegisterForClicks("LeftButtonUp","RightButtonUp");
	    </OnLoad>
		<OnClick>
			if button == "LeftButton" then
				TRPPlaySoundGlobal(getglobal(this:GetName().."Url"):GetText(),IsControlKeyDown(),true);
			end
			TRPPlaySound(getglobal(this:GetName().."Url"):GetText());
		</OnClick>
	</Scripts>
</Button>

<!-- Emplacement Modele TEMPLATE  -->
<Button name="ModeleSlotTemplate" virtual="true">
   <Size>
      <AbsDimension x="20" y="20"/>
    </Size>
    <Layers>
      <Layer level="BORDER">
        <Texture name="$parentIcon" file="Interface\Buttons\UI-EmptySlot-Disabled">
		  <Size>
			<AbsDimension x="20" y="20"/>
		  </Size>
		  <Anchors>
			<Anchor point="CENTER">
			  <Offset>
				<AbsDimension x="-1" y="0"/>
			  </Offset>
			</Anchor>
		  </Anchors>
		</Texture>
      </Layer>
	  <Layer level="OVERLAY">
	  </Layer>
    </Layers>
    <NormalTexture name="$parentNormalTexture" file="Interface\Buttons\UI-Quickslot2">
      <Size>
        <AbsDimension x="35" y="35"/>
      </Size>
      <Anchors>
        <Anchor point="CENTER">
          <Offset>
            <AbsDimension x="0" y="-1"/>
          </Offset>
        </Anchor>
      </Anchors>
    </NormalTexture>
    <PushedTexture file="Interface\Buttons\UI-Quickslot-Depress"/>
    <HighlightTexture alphaMode="ADD" file="Interface\Buttons\ButtonHilight-Square"/>
	<Scripts>
		<OnEnter>
			GameTooltip:SetOwner(this, "ANCHOR_TOPLEFT");
			GameTooltip:Show();
		</OnEnter>
		<OnLeave>
			GameTooltip:Hide();
		</OnLeave>
		<OnLoad>
	       this:RegisterForClicks("LeftButtonUp","RightButtonUp");
	    </OnLoad>
		<OnClick>
			if TRPListeModele.frame then
				TRPListeModele.frame.model = this.Nom;
			end
			TRPListeModele:Hide();
		</OnClick>
	</Scripts>
</Button>

</Ui>