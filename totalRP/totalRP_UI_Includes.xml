				</Size>
				<Anchors>
					<Anchor point="CENTER">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Color r="0.95" g="0.95" b="0.95"/>
			</FontString>
		</Layer>
	</Layers>
	<Frames>
		<Button name="$parentBoutonPlus" virtual="true">
		   <Size>
		      <AbsDimension x="15" y="15"/>
		    </Size>
			<Anchors>
				<Anchor point="RIGHT">
					<Offset>
						<AbsDimension x="5" y="-10"/>
					</Offset>
				</Anchor>
			</Anchors>
		    <NormalTexture name="$parentNormalTexture" file="Interface\Buttons\UI-PlusButton-Up"/>
		    <PushedTexture file="Interface\Buttons\UI-PlusButton-Down"/>
		    <HighlightTexture alphaMode="ADD" file="Interface\Buttons\ButtonHilight-Square"/>
			<Scripts>
				<OnLeave>
					GameTooltip:Hide()
				</OnLeave>
				<OnEnter>
					GameTooltip:SetOwner(this, "ANCHOR_TOPRIGHT");
					GameTooltip:AddLine("+1", 1, 1, 1);
					GameTooltip:Show();
				</OnEnter>
				<OnClick>
					local min,max = this:GetParent():GetMinMaxValues();
					local value = this:GetParent():GetValue();
					if value == max then
					
					else
						this:GetParent():SetValue(this:GetParent():GetValue()+1);
					end
				</OnClick>
				<OnUpdate>
					if this:GetParent():IsEnabled() then
						this:Enable();
					else
						this:Disable();
					end
				</OnUpdate>
			</Scripts>
		</Button>
