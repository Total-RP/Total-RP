<!-- Template de la boite de saisie  -->
<EditBox name="SaisieTemplate" letters="150" historyLines="10" autoFocus="false" virtual="true">
	<Size>
		<AbsDimension x="185" y="18"/>
	</Size>
	<Layers>
		<Layer level="BACKGROUND">
			<Texture name="$parentLeft" file="Interface\Common\Common-Input-Border">
				<Size>
					<AbsDimension x="12" y="28"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="-6" y="-1"/>
						</Offset>
					</Anchor>
				</Anchors>
				<TexCoords left="0" right="0.09375" top="0" bottom="1.0"/>
			</Texture>
			<Texture name="$parentCenter" file="Interface\Common\Common-Input-Border">
				<Size>
					<AbsDimension x="200" y="28"/>
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentLeft" relativePoint="RIGHT"/>
				</Anchors>
				<TexCoords left="0.09375" right="0.90625" top="0" bottom="1.0"/>
			</Texture>
			<Texture name="$parentRight" file="Interface\Common\Common-Input-Border">
				<Size>
					<AbsDimension x="12" y="28"/>
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentCenter" relativePoint="RIGHT"/>
				</Anchors>
				<TexCoords left="0.90625" right="1.0" top="0" bottom="1.0"/>
			</Texture>
		</Layer>
		<Layer level="OVERLAY">
			<FontString name="$parentTexte" inherits="GameFontNormalSmall" justifyH="CENTER" text="text">
				<Size>
					<AbsDimension x="500" y="0"/>
				</Size>
				<Anchors>
					<Anchor point="CENTER">
						<Offset>
							<AbsDimension x="85" y="7"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Color r="0.95" g="0.95" b="0.95"/>
			</FontString>
		</Layer>
	</Layers>
	<FontString inherits="GameFontWhite"/>
	<Scripts>
		<OnEscapePressed>
			this:ClearFocus()
		</OnEscapePressed>
		<OnUpdate>
			if this.max then
				getglobal(this:GetName().."Texte"):Show();
				local actual = string.len(this:GetText());
				getglobal(this:GetName().."Texte"):SetText("|cffffaa00("..actual.."/"..this.max..")");
				if actual > this.max then
					this:SetText(string.sub(this:GetText(),1,this.max));
				end
			else
				getglobal(this:GetName().."Texte"):Hide();
			end
		</OnUpdate>
	</Scripts>
</EditBox>

